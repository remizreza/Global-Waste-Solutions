{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "headers": [
    {
      "source": "/assets/hero-bg-1200.mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
